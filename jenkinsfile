def abc = sh 'git tag | xargs -I@ git log --format=format:"%ai @%n" -1 @ | sort -k2 | tail -n 1 | awk '{print $4}''
pipeline {
    agent any
    environment {
          MY_FILES = "sh(script: 'cd mydir && ls -l', returnStdout: true)"
        }
    stages {
        stage ('Test') {
            steps {
                echo "${MY_FILES}"
                echo "${abc}"
                }
            }
        }
    }