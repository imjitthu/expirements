pipeline {
    agent any
    environment {
        TAG = "tag"
        //TAG = sh 'git tag | xargs -I@ git log --format=format:"%ai @%n" -1 @ | sort -k2 | tail -n 1 | awk '{print $4}''
    }
    stages {
        stage ('Test') {
            steps {
                echo "${env.TAG}"
                script {
                def GIT_TAG = sh(script: 'git tag | xargs -I@ git log --format=format:"%ai @%n" -1 @ | sort -k2 | tail -n 1 | awk '{print $4}'', returnStdout: true)
                println GIT_TAG
                }
            }
        }
    }
}