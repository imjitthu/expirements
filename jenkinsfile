pipeline {
    agent any
    environment {
        TAG = sh 'tag=$(git tag | xargs -I@ git log --format=format:"%ai @%n" -1 @ | sort -k2 | tail -n 1 | awk '{print $4}')'
    }
    stages {
        stage ('Test') {
            steps {
                echo "${env.TAG}"
                sh 'cat pod.yml'
            }
        }
    }
}